<div class="nested-fields">
    <div class="field">
        <%= f.label :strain_id %>
        <%= f.select :strain_id, options_from_collection_for_select(@strains, :id, :name) %>
    </div>
    <div class="field">
        <%= f.label :percentage %>
        <%= f.number_field :percentage %>    
    </div>
    <%= link_to_remove_association "remove strain", f %>
</div>